<%user =  @user_current%>
<% posts = @user_current.last_posts %>

<%= render partial: "renders/user", object: user%>

<div class="bio">
<h2>bio</h2>
<p class="bio-text"> <%= user.bio%> </p>
</div>

<div class="post-father">
<ul class="post-wrap">
   <%= render partial: "renders/post", collection: posts%>
</ul>
<%= link_to "See all posts", "/users/#{user.id}/posts" %></div>

