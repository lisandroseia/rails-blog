<%user =  @user_current%>
<% posts = @user_current.last_posts %>

<%= render partial: "renders/user", object: user%>

<div class="bio">
<h2>bio</h2>
<p class="bio-text"> <%= user.bio%> </p>
</div>

<div>
<ul class="post-wrap">
<% posts.each_with_index do |post, index|%>
<li class="post-data">
    <p>post #<%= index + 1%></p>
   <%= render partial: "renders/post", object: post%>
</li>
<%end%>
</ul>
</div>

